{
  "expo": {
    "name": "StyleGuide",
    "plugins": [
      "expo-camera"
    ],
    "slug": "StyleGuide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/style-guide-text.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F1DEDE"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/style-guide-text.png"
    }
  }
}
